<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xboost.mapper.SiteInfoMapper">

    <cache/>

    <insert id="save">
        INSERT into t_site_info(site_code,longitude,latitude,site_name,site_address,site_nightDelivery,site_area,site_type,car_num,
        large_carModle,max_operate_num,str1,str2,str3,createtime,updatetime)
        VALUES (#{siteCode},#{siteLongitude},#{siteLatitude},#{siteName},#{siteAddress},#{siteNightDelivery},#{siteArea},#{siteType},
        #{carNum},#{largeCarModel},#{maxOperateNum},#{str1},#{str2},#{str3},#{createTime},#{updateTime})
    </insert>


</mapper>