<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xboost.mapper.SolutionMapper">

    <cache/>
    <insert id="addRoute">
        INSERT into t_solution_route(scenariosId ,route_count,car_type,location,sequence,cur_loc,
        type,sb_loc,sb_vol,arr_time,end_time,unload_loc,unload_vol,next_cur_loc,calc_dis,
        car_goods,createtime,updatetime)
        VALUES (#{scenariosId},#{routeCount},#{carType},#{location},#{sequence},#{curLoc},
        #{type},#{sbLoc},#{sbVol},#{arrTime},#{endTime},#{unloadLoc},#{unloadVol},#{nextCurLoc},#{calcDis},
        #{carGoods},#{createTime},#{updateTime})
    </insert>

    <resultMap id="modelRoute" type="Route">
        <result property="id" column="id"/>
        <result property="scenariosId" column="scenariosId"/>
        <result property="parameterName" column="parameter_name"/>
        <result property="data" column="data"/>
        <result property="note" column="note"/>
    </resultMap>

</mapper>